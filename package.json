{
	"name": "my-finance",
	"version": "1.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"css": "node ./scripts/css.js",
		"sitemap": "node ./scripts/sitemap.js",
		"locales": "node ./scripts/locales.js",
		"dev": "npm run locales && next dev -p 3020",
		"build": "npm run locales && next build && npm run css",
		"start": "next start -p 3020",
		"size": "npx cross-env ANALYZE=true npm run build",
		"ts-validity": "tsc --noEmit",
		"lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
		"lint:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
		"cypress:open": "cypress open",
		"e2e": "cypress open --e2e",
		"e2e:headless": "cypress run --e2e",
		"component": "cypress open --component",
		"component:headless": "cypress run --component"
	},
	"dependencies": {
		"@ant-design/nextjs-registry": "^1.0.0",
		"@ducanh2912/next-pwa": "^10.2.7",
		"@reduxjs/toolkit": "^2.2.5",
		"@sentry/nextjs": "^7.114.0",
		"@supabase/supabase-js": "^2.43.4",
		"antd": "^5.17.3",
		"i18next": "^23.11.5",
		"i18next-resources-to-backend": "^1.2.1",
		"next": "^14.2.3",
		"next-i18n-router": "^5.4.3",
		"nodemailer": "^6.9.13",
		"query-string": "^9.0.0",
		"react": "^18",
		"react-dom": "^18",
		"react-google-recaptcha": "^3.1.0",
		"react-i18next": "^14.1.1",
		"react-loader-spinner": "^6.1.6",
		"react-redux": "^9.1.2",
		"recharts": "^2.12.7",
		"redux": "^5.0.1"
	},
	"devDependencies": {
		"@cypress/webpack-preprocessor": "^6.0.1",
		"@next/bundle-analyzer": "^14.2.3",
		"@svgr/webpack": "^8.1.0",
		"@types/node": "^20",
		"@types/react": "^18.3.2",
		"@types/react-dom": "^18",
		"autoprefixer": "^10.4.19",
		"cypress": "^13.10.0",
		"dotenv": "^16.4.5",
		"eslint": "^8",
		"eslint-config-next": "14.2.3",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"i18nexus-cli": "^3.4.0",
		"postcss": "^8",
		"prettier": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"schema-dts": "^1.1.2",
		"sitemap": "^7.1.1",
		"tailwindcss": "^3.4.3",
		"ts-loader": "^9.5.1",
		"typescript": "^5",
		"webpack": "^5.91.0"
	}
}
