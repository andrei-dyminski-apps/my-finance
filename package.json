{
	"name": "my-finance",
	"version": "1.0.0",
	"type": "module",
	"private": true,
	"scripts": {
		"css": "node --loader ts-node/esm ./scripts/css.ts",
		"sitemap": "node --loader ts-node/esm ./scripts/sitemap.ts",
		"locales": "node --loader ts-node/esm ./scripts/locales.ts",
		"dev": "npx kill-port 3020 && npm run locales && next dev -p 3020",
		"build": "npm run locales && next build && npm run css",
		"start": "npx kill-port 3020 && next start -p 3020",
		"size": "npx cross-env ANALYZE=true npm run build",
		"typescript": "tsc --noEmit",
		"eslint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
		"eslint:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
		"stylelint": "stylelint \"**/*.{css,scss,less,styl}\"",
		"cypress:open": "cypress open",
		"e2e": "cypress open --e2e",
		"e2e:headless": "cypress run --e2e",
		"component": "cypress open --component",
		"component:headless": "cypress run --component"
	},
	"dependencies": {
		"@ant-design/nextjs-registry": "^1.0.1",
		"@ducanh2912/next-pwa": "^10.2.9",
		"@reduxjs/toolkit": "^2.2.7",
		"@sentry/nextjs": "^8.30.0",
		"@supabase/supabase-js": "^2.45.3",
		"@vercel/speed-insights": "^1.0.12",
		"antd": "^5.20.5",
		"i18next": "^23.14.0",
		"i18next-resources-to-backend": "^1.2.1",
		"lodash": "^4.17.21",
		"next": "^14.2.13",
		"next-i18n-router": "^5.5.1",
		"nodemailer": "^6.9.15",
		"query-string": "^9.1.0",
		"react": "^18",
		"react-dom": "^18",
		"react-google-recaptcha": "^3.1.0",
		"react-i18next": "^15.0.2",
		"react-loader-spinner": "^6.1.6",
		"react-redux": "^9.1.2",
		"recharts": "^2.12.7",
		"redux": "^5.0.1"
	},
	"devDependencies": {
		"@cypress/webpack-preprocessor": "^6.0.2",
		"@faker-js/faker": "^8.4.1",
		"@next/bundle-analyzer": "^14.2.11",
		"@svgr/webpack": "^8.1.0",
		"@types/fs-extra": "^11.0.4",
		"@types/lodash": "^4.17.7",
		"@types/node": "^22",
		"@types/nodemailer": "^6.4.16",
		"@types/react": "^18.3.5",
		"@types/react-dom": "^18",
		"@types/react-google-recaptcha": "^2.1.9",
		"@types/recharts": "^1.8.29",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"autoprefixer": "^10.4.20",
		"cheerio": "^1.0.0",
		"cypress": "^13.14.2",
		"cypress-terminal-report": "^6.2.0",
		"dotenv": "^16.4.5",
		"eslint": "^8",
		"eslint-config-next": "14.2.15",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"i18nexus-cli": "^3.5.0",
		"postcss": "^8",
		"prettier": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"schema-dts": "^1.1.2",
		"sitemap": "^8.0.0",
		"stylelint": "^16.9.0",
		"stylelint-prettier": "^5.0.2",
		"tailwindcss": "^3.4.12",
		"ts-essentials": "^10.0.2",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"typescript": "^5",
		"webpack": "^5.94.0"
	}
}
